<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <%- include('./partials/head.ejs', {title: 'Bird Details | NestQuest' }) %>
        <title>NestQuest | Bird Details</title>
        <%- include('./partials/nav.ejs') %>

</head>

<body>
    <main>
        <h1>Details</h1>

        <ul>
            <% birds.forEach((bird)=> { %>
                <li>
                    <%= bird.comName %>
                        <span style="font-style: italic;">
                            <%= bird.sciName %>
                        </span>
                </li>
                <li>
                    <%= bird.familyComName %>
                </li>
                <% }); %>

        </ul>
    </main>
    <%- include('./partials/footer.ejs') %>
</body>

</html>