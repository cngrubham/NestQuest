<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head.ejs', {title: 'NestQuest | Birds by Region' }) %>
        <link rel="stylesheet" href="/styles/home.css">
        <%- include('./partials/nav.ejs') %>

</head>


<body>
    <main>
        <h1>Birds by Region</h1>
        <ul>
            <% birds.forEach((bird)=> { %>
                <li>
                    <p>
                        <%= bird.comName %>
                            <span style="font-style: italic;">
                                <%= bird.sciName %>
                            </span>
                    </p>
                </li>
                <% }); %>

        </ul>
    </main>
    <%- include('./partials/footer.ejs') %>
</body>

</html>